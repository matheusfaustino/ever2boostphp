{
  "name": "matheusfaustino/ever2boostphp",
  "description": "CLI App that helps you import you notes from evernote to boostnote.",
  "license": "MIT",
  "readme": "README.md",
  "type": "library",
  "authors": [
    {
      "name": "Matheus Faustino",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/matheusfaustino/ever2boostphp/issues"
  },
  "keywords": [
    "evernote",
    "import",
    "boostnote"
  ],
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "adrienrn/php-mimetyper": "^0.2.1",
    "evernote/evernote-cloud-sdk-php": "^2.0",
    "ramsey/uuid": "^2.9",
    "react/http": "^0.8.3",
    "symfony/console": "^4.1",
    "symfony/filesystem": "^4.1",
    "symfony/process": "^4.1"
  },
  "require-dev": {
    "phpro/grumphp": "^0.14.2",
    "sebastian/phpcpd": "^4.0",
    "squizlabs/php_codesniffer": "^3.3",
    "phpmd/phpmd": "^2.6",
    "maglnet/composer-require-checker": "^1.1",
    "jakub-onderka/php-parallel-lint": "^1.0",
    "sensiolabs/security-checker": "^4.1",
    "phpstan/phpstan": "^0.10.3"
  },
  "autoload": {
    "psr-4": {
      "Ever2BoostPHP\\": "src/"
    }
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "bin": [
    "ever2boostphp"
  ]
}
